import java.util.*;
public class Main{
public static void main (String... args){
Scanner object = new Scanner (System.in);
System.out.print("enter the no for fibonachi");
int n = object.nextInt();
int a =0;
int b = 1;
for (i = 0 ; i <= n ; i++){
System.out.print(a);
int c = a +b;
a = b;
b = c;
}
}
}
