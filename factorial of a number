import java.util.*;
public class Main{
public static void main(String... args){
Scanner object = new Scanner(System.in);

System.out.print("enter the number for factorial");
int n = object.nextInt();

int factorial = 1;
for(int i = 0 ; i < n ; i++){
factorial*=i;
}
System.out.print("factorial of the number is " + factorial);
}
}

        
            


